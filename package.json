  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "electron:publish": "electron-builder -p always",
    "electron:build": "npm run build && electron-builder",
    "build-win": "npm run build && electron-builder build --win --x64",
    "create-sdk": "cd src/core/scripts && bash create-sdk.sh",
    "update-sdk": "cd src/core/scripts && bash update-sdk.sh",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "yarn eslint && yarn stylelint",
    "eslint": "eslint . --cache",
    "stylelint": "stylelint '**/*.css' --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "yarn typecheck && yarn prettier && yarn lint && yarn vitest && yarn build"
